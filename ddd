local WallButton = MainTab:CreateButton({
    Name = "ë²½ ëš«ê¸° í† ê¸€(ì‹œì‘/ì¢…ë£Œ)ğŸ³",
    Callback = function()
        local player = game:GetService("Players").LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()

        -- ë²½ í†µê³¼ ìƒíƒœ ì „í™˜
        if not canPassThroughWalls then
            canPassThroughWalls = true
            -- ëª¨ë“  íŒŒíŠ¸ì˜ CanCollideë¥¼ ë¹„í™œì„±í™”
            for _, part in ipairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false  -- ë²½ ëš«ê¸° í™œì„±í™”
                end
            end
            game.StarterGui:SetCore("SendNotification", {
                Title = "ë²½ í†µê³¼ ì‹œì‘ğŸ³",
                Text = "ë²½ì„ ëš«ì„ ìˆ˜ ìˆìŠµë‹ˆë‹¤.ğŸ³",
                Duration = 5
            })
        else
            canPassThroughWalls = false
            -- ëª¨ë“  íŒŒíŠ¸ì˜ CanCollideë¥¼ í™œì„±í™”
            for _, part in ipairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = true  -- ë²½ í†µê³¼ ë¹„í™œì„±í™”
                end
            end
            game.StarterGui:SetCore("SendNotification", {
                Title = "ë²½ í†µê³¼ ì¢…ë£ŒğŸ³",
                Text = "ë²½ì„ ë” ì´ìƒ ëš«ì„ ìˆ˜ ì—†ìŠµë‹ˆë‹¤.ğŸ³",
