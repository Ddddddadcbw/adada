            isFlying = true

            -- BodyVelocity ì¸ìŠ¤í„´ìŠ¤ ìƒì„± (í”Œë ˆì´ì–´ë¥¼ ê³µì¤‘ìœ¼ë¡œ ë„ìš°ê¸° ìœ„í•´)
            bodyVelocity = Instance.new("BodyVelocity")
            bodyVelocity.MaxForce = Vector3.new(400000, 400000, 400000) -- í˜ì˜ í¬ê¸° ì„¤ì • (ë¹„í–‰ì„ ìœ„í•œ ìµœì†Œê°’)
            bodyVelocity.Velocity = Vector3.new(0, flyingSpeed, 0) -- ê¸°ë³¸ ë¹„í–‰ ì†ë„ ì„¤ì • (ìˆ˜ì§ìœ¼ë¡œ ë„ìš´ë‹¤)
            bodyVelocity.Parent = character:WaitForChild("HumanoidRootPart")

            -- ì•Œë¦¼ í‘œì‹œ
            game.StarterGui:SetCore("SendNotification", {
                Title = "ë¹„í–‰ ì‹œì‘",
                Text = "ì´ì œ ë¹„í–‰í•  ìˆ˜ ìˆìŠµë‹ˆë‹¤.",
                Duration = 5
            })
        else
            isFlying = false

            -- ë¹„í–‰ì„ ì¢…ë£Œí•˜ê³  BodyVelocityë¥¼ ì œê±°
            if bodyVelocity then
                bodyVelocity:Destroy()
            end

            -- ì•Œë¦¼ í‘œì‹œ
            game.StarterGui:SetCore("SendNotification", {
                Title = "ë¹„í–‰ ì¢…ë£Œ",
                Text = "ë¹„í–‰ì´ ì¢…ë£Œë˜ì—ˆìŠµë‹ˆë‹¤.",
                Duration = 5
            })
        end
    end,
})

-- ë¹„í–‰ ì†ë„ ì¡°ì • ìŠ¬ë¼ì´ë”
local FlySpeedSlider = FlyTab:CreateSlider({
    Name = "ë¹„í–‰ ì†ë„",
    Range = {10, 100},
    Increment = 1,
    Suffix = "Speed",
    CurrentValue = flyingSpeed,
    Flag = "flyspeed", -- ì†ë„ ì¡°ì • ì‹œ ì €ì¥ë˜ëŠ” ê°’
    Callback = function(Value)
        flyingSpeed = Value
        if isFlying and bodyVelocity then
            bodyVelocity.Velocity = Vector3.new(0, flyingSpeed, 0)
        end
    end,
})

-- ìƒìŠ¹ ë²„íŠ¼ (Eë¡œ ì´ë¥™)
local AscendButton = FlyTab:CreateButton({
    Name = "ì´ë¥™ (ìƒìŠ¹)",
    Callback = function()
        if isFlying and bodyVelocity then
            -- ìƒìŠ¹ ì†ë„ ì ìš©
            isAscending = true
            bodyVelocity.Velocity = Vector3.new(0, ascendSpeed, 0)
            game.StarterGui:SetCore("SendNotification", {
                Title = "ìƒìŠ¹ ì‹œì‘",
                Text = "ë¹„í–‰ì´ ìƒìŠ¹ ëª¨ë“œë¡œ ë³€ê²½ë˜ì—ˆìŠµë‹ˆë‹¤.",
                Duration = 5
            })
        else
            game.StarterGui:SetCore("SendNotification", {
                Title = "ë¹„í–‰ ì‹œì‘ í•„ìš”",
                Text = "ë¹„í–‰ì„ ë¨¼ì € ì‹œì‘í•´ì•¼ í•©ë‹ˆë‹¤.",
                Duration = 5
            })
        end
    end,
})

-- í•˜ê°• ë²„íŠ¼ (Që¡œ í•˜ê°•)
local FlyTab = Window:CreateTab("Fly System", nil) -- ìƒˆë¡œìš´ íƒ­ ìƒì„±

-- ë¹„í–‰ ìƒíƒœ ë° ì´ë™ ì†ë„ ì„¤ì •
local isFlying = false
local flyingSpeed = 50
local ascendSpeed = 25
local descendSpeed = -25
local moveSpeed = 5 -- ì¡°ê¸ˆì”© ì›€ì§ì´ë„ë¡ ë‚®ì€ ê°’ ì„¤ì •

-- ë¹„í–‰ì„ ìœ„í•œ BodyVelocity ì¸ìŠ¤í„´ìŠ¤
local bodyVelocity
local targetVelocity = Vector3.new(0, 0, 0) -- ëª©í‘œ ì†ë„ ì €ì¥ìš©

-- ë¹„í–‰ í† ê¸€ ë²„íŠ¼
local FlyButton = FlyTab:CreateButton({
    Name = "ë¹„í–‰ í† ê¸€",
    Callback = function()
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()

        if not isFlying then
            isFlying = true
            bodyVelocity = Instance.new("BodyVelocity")
            bodyVelocity.MaxForce = Vector3.new(400000, 400000, 400000)
            bodyVelocity.Velocity = Vector3.new(0, 0, 0)
            bodyVelocity.Parent = character:WaitForChild("HumanoidRootPart")

            game.StarterGui:SetCore("SendNotification", {
                Title = "ë¹„í–‰ ì‹œì‘",
                Text = "ì´ì œ ë¹„í–‰í•  ìˆ˜ ìˆìŠµë‹ˆë‹¤.",
                Duration = 5
            })

            -- ì§€ì†ì ì¸ ì†ë„ ì—…ë°ì´íŠ¸
            spawn(function()
                while isFlying do
                    if bodyVelocity then
                        bodyVelocity.Velocity = bodyVelocity.Velocity:Lerp(targetVelocity, 0.1) -- ë¶€ë“œëŸ¬ìš´ ì†ë„ ë³€í™”
                    end
                    wait(0.1) -- 0.1ì´ˆë§ˆë‹¤ ê°±ì‹ 
                end
            end)
        else
            isFlying = false
            if bodyVelocity then
                bodyVelocity:Destroy()
            end
            game.StarterGui:SetCore("SendNotification", {
                Title = "ë¹„í–‰ ì¢…ë£Œ",
                Text = "ë¹„í–‰ì´ ì¢…ë£Œë˜ì—ˆìŠµë‹ˆë‹¤.",
                Duration = 5
            })
        end
    end,
})

-- í‚¤ ì…ë ¥ ì²˜ë¦¬
game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
    if not isFlying then return end

    local player = game.Players.LocalPlayer
    local character = player.Character

    if input.UserInputType == Enum.UserInputType.Keyboard then
        if input.KeyCode == Enum.KeyCode.W then
            targetVelocity = targetVelocity + character.HumanoidRootPart.CFrame.LookVector * moveSpeed
        elseif input.KeyCode == Enum.KeyCode.S then
            targetVelocity = targetVelocity - character.HumanoidRootPart.CFrame.LookVector * moveSpeed
        elseif input.KeyCode == Enum.KeyCode.A then
            targetVelocity = targetVelocity - character.HumanoidRootPart.CFrame.RightVector * moveSpeed
        elseif input.KeyCode == Enum.KeyCode.D then
            targetVelocity = targetVelocity + character.HumanoidRootPart.CFrame.RightVector * moveSpeed
        elseif input.KeyCode == Enum.KeyCode.E then
            targetVelocity = targetVelocity + Vector3.new(0, ascendSpeed, 0) -- ìƒìŠ¹
        elseif input.KeyCode == Enum.KeyCode.Q then
            targetVelocity = targetVelocity + Vector3.new(0, descendSpeed, 0) -- í•˜ê°•
        end
    end
end)

-- í‚¤ë¥¼ ë—„ ë•Œ ì´ë™ ë©ˆì¶”ê¸°
game:GetService("UserInputService").InputEnded:Connect(function(input, gameProcessed)
    if not isFlying then return end

    if input.UserInputType == Enum.UserInputType.Keyboard then
        if input.KeyCode == Enum.KeyCode.W or input.KeyCode == Enum.KeyCode.S then
            targetVelocity = Vector3.new(0, targetVelocity.Y, 0) -- ìˆ˜ì§ ì†ë„ ìœ ì§€, ìˆ˜í‰ ì†ë„ ì œê±°
        elseif input.KeyCode == Enum.KeyCode.A or input.KeyCode == Enum.KeyCode.D then
            targetVelocity = Vector3.new(0, targetVelocity.Y, 0) -- ìˆ˜ì§ ì†ë„ ìœ ì§€, ìˆ˜í‰ ì†ë„ ì œê±°
        elseif input.KeyCode == Enum.KeyCode.E or input.KeyCode == Enum.KeyCode.Q then
            targetVelocity = Vector3.new(targetVelocity.X, 0, targetVelocity.Z) -- ìˆ˜í‰ ì†ë„ ìœ ì§€, ìˆ˜ì§ ì†ë„ ì œê±°
        end
    end
end)
