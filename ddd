-- ë¹„í–‰ ì¤‘ì— ë°©í–¥ ì¡°ì •(ì•, ë’¤, ì¢Œ, ìš°)
local function updateFlightDirection()
    if not isFlying or not bodyVelocity then return end

    local player = game.Players.LocalPlayer
    local character = player.Character
    local mouse = game:GetService("Players").LocalPlayer:GetMouse()

    -- ë§ˆìš°ìŠ¤ ìœ„ì¹˜ë¥¼ ê¸°ì¤€ìœ¼ë¡œ ì´ë™ ë°©í–¥ ì„¤ì •
    local mousePos = mouse.Hit.p
    local direction = (mousePos - character.HumanoidRootPart.Position).unit

    -- í‚¤ ì…ë ¥ì„ í†µí•œ ì•ë’¤, ì¢Œìš° ì´ë™
    local moveDirection = Vector3.new(0, 0, 0)
    if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.W) then
        moveDirection = moveDirection + character.HumanoidRootPart.CFrame.LookVector -- ì•ìœ¼ë¡œ ì´ë™
    elseif game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.S) then
        moveDirection = moveDirection - character.HumanoidRootPart.CFrame.LookVector -- ë’¤ë¡œ ì´ë™
    end
    if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.A) then
        moveDirection = moveDirection - character.HumanoidRootPart.CFrame.RightVector -- ì™¼ìª½ìœ¼ë¡œ ì´ë™
    elseif game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.D) then
        moveDirection = moveDirection + character.HumanoidRootPart.CFrame.RightVector -- ì˜¤ë¥¸ìª½ìœ¼ë¡œ ì´ë™
    end

    -- ìµœì¢…ì ìœ¼ë¡œ ì´ë™ ë°©í–¥ ê³„ì‚° (ìˆ˜ì§ ë¹„í–‰ ì†ë„ + ìˆ˜í‰ ì´ë™)
    bodyVelocity.Velocity = direction * flyingSpeed + moveDirection * moveSpeed
end

-- ë¹„í–‰ ì¤‘ì— ë§ˆìš°ìŠ¤ ë° í‚¤ ì…ë ¥ ì²˜ë¦¬
game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
    if not isFlying then return end

    if input.UserInputType == Enum.UserInputType.MouseMovement then
        -- ë§ˆìš°ìŠ¤ë¥¼ ë”°ë¼ê°€ëŠ” ë¹„í–‰
        updateFlightDirection()
    end

    if input.UserInputType == Enum.UserInputType.Keyboard then
        -- WASD í‚¤ë¡œ ì´ë™
        updateFlightDirection()
    end
end)

game:GetService("UserInputService").InputChanged:Connect(function(input, gameProcessed)
    if not isFlying then return end
    if input.UserInputType == Enum.UserInputType.MouseMovement then
        -- ë§ˆìš°ìŠ¤ê°€ ì´ë™í•  ë•Œë§ˆë‹¤ ë¹„í–‰ ë°©í–¥ ê°±ì‹ 
        updateFlightDirection()
    end
end)

